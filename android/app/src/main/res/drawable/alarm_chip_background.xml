<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:startColor="@color/alarm_chip_background"
                android:endColor="@color/alarm_chip_background" />
            <corners android:radius="18dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/alarm_chip_stroke" />
            <corners android:radius="18dp" />
        </shape>
    </item>
</layer-list>
